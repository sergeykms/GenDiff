{
    "name": "hexlet/code",
    "description": "GenDiff",
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Sergeykms\\HexletPhp\\": "src/"
        },
        "files": [
            "src/autoload.php",
            "src/Differ/Differ.php",
            "src/Differ/Parser.php",
            "src/Formatters/Stylish.php",
            "src/Formatters/Plain.php",
            "src/Formatters.php"
        ]
    },
    "bin": [
        "bin/gendiff"
      ],
    "authors": [
        {
            "name": "Popov Sergey",
            "email": "serpp@me.com"
        }
    ],
    "minimum-stability": "dev",
    "require-dev": {
        "psy/psysh": "@stable",
        "phpstan/phpstan-phpunit": "^1.2.2",
        "phpstan/extension-installer": "^1.2",
        "symfony/var-dumper": "^6.2.1",
        "phpunit/phpunit": "^9.5.26",
        "squizlabs/php_codesniffer": "^3.7.1"
    },
    "require": {
        "docopt/docopt": "dev-master",
        "tightenco/collect": "^9.33",
        "nesbot/carbon": "^2.64",
        "symfony/string": "^6.2",
        "phpstan/phpstan": "^1.9.2",
        "symfony/yaml": "6.4.x-dev"
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
